遍历二维数组

方法一：
public class Test {

	public static void main(String[] args) {
	
		double[][] prob= new double[10][20];
		for (int i = 0; i < prob.length; i++) {
			for (int j = 0; j < prob[i].length; j++) {
				System.out.print(prob[i][j] );
			}
		}
	}

}


方法二：
public class Test {

	public static void main(String[] args) {
	
		double[][] prob= new double[10][20];
		for (double[] pArray : prob) {
			for (double p : pArray) {
				System.out.print(p);
			}
		}
	}
}

结果均为：
0.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.00.0
